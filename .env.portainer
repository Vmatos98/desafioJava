# Configurações para Portainer
# Use portas que provavelmente estão livres

# Portas principais (tente estas primeiro)
FRONTEND_PORT=3001
BACKEND_PORT=8091

# Portas alternativas (caso as principais estejam ocupadas)
# FRONTEND_PORT=3002
# BACKEND_PORT=8092

# Ou use portas altas (menos chance de conflito)
# FRONTEND_PORT=9001
# BACKEND_PORT=9091

# Configurações do Backend
JAVA_OPTS=-Xmx1g -Xms512m
SPRING_PROFILES_ACTIVE=docker

# Nome do projeto (para evitar conflitos)
COMPOSE_PROJECT_NAME=aprovados